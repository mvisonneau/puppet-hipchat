---
# Managed with Puppet
:hipchat_token: '<%= @token %>'
:hipchat_room: '<%= @room %>'
:hipchat_notify_color: <%= @notify_color %>'
:hipchat_notify_room: <%= @hipchat_notify_room %>
:hipchat_statuses:
<% @statuses.each do |status| %>
  - <%= status %>
<% end %>
:hipchat_puppetboard: <%= @hipchat_puppetboard %>
:hipchat_dashboard: <%= @hipchat_dashboard %>
:hipchat_proxy: '<%= @hipchat_proxy %>'
:hipchat_failed_color: '<%= @hipchat_failed_color %>'
:hipchat_successful_color: '<%= @hipchat_successful_color %>'
:hipchat_unchanged_color: '<%= @hipchat_unchanged_color %>'